    <span class="p">{</span>
        <span class="k">if</span><span class="p">(</span><span class="n">num</span><span class="p">[</span><span class="n">j</span><span class="p">]</span><span class="o">==</span><span class="mi">0</span><span class="p">)</span>
            <span class="k">continue</span><span class="p">;</span>
