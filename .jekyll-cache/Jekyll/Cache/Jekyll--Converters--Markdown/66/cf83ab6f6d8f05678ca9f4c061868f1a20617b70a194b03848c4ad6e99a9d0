            <span class="p">{</span>
                <span class="n">sum</span><span class="p">[</span><span class="mi">2</span><span class="o">*</span><span class="n">j</span><span class="o">+</span><span class="mi">2</span><span class="p">]</span><span class="o">++</span><span class="p">;</span>
